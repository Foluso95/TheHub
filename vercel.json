{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/api/static/$1"
    },
    {
      "src": "/robots.txt",
      "dest": "/api/static/robots.txt"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
